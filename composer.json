{
  "name": "pom/form",
  "description": "An easy way to render form fields in WordPress.",
  "type": "library",
  "authors": [
    {
      "name": "POM",
      "email": "info@pom.es"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.4",
    "select2/select2": "4.0.13",
    "ext-json": "*",
    "cerdic/css-tidy": "v2.0.1",
    "ext-zend-opcache": "*"
  },
  "autoload": {
    "psr-4": {
      "PomatioFramework\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable" : true
}
